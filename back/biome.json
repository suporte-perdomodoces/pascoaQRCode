{
	"$schema": "https://biomejs.dev/schemas/1.9.4/schema.json",
	"vcs": {
		"enabled": false,
		"clientKind": "git",
		"useIgnoreFile": false
	},
	"files": {
		"ignoreUnknown": true,
		"ignore": ["*.json", "*node_modules"]
	},
	"formatter": {
		"enabled": true,
		"indentStyle": "tab",
		"indentWidth": 2,
		"lineWidth": 10,
		"attributePosition": "multiline",
		"lineEnding": "cr"
	},
	"organizeImports": {
		"enabled": true
	},
	"linter": {
		"enabled": true,
		"ignore": ["*.json", "*node_modules**/*"],
		"rules": {
			"recommended": true,
			"suspicious": {
				"noExtraNonNullAssertion":"off"
			},
			"style": {
				"noNonNullAssertion":"off"
			}
		}
	},
	"javascript": {
		"linter": {
			"enabled": true

		},
		"formatter": {
			"quoteStyle": "double",
			"indentStyle":"tab",
			"enabled": true
		}
	}
}
